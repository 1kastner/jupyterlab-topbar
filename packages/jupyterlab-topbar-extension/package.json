{
  "name": "jupyterlab-topbar-extension",
  "version": "0.5.0",
  "description": "JupyterLab extension to expose the top bar space",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jtpio/jupyterlab-topbar",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlab-topbar"
  },
  "license": "BSD-3-Clause",
  "author": "Jeremy Tuloup",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlab-topbar.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-rc.6",
    "@jupyterlab/apputils": "^3.0.0-rc.6",
    "@jupyterlab/coreutils": "^5.0.0-rc.6",
    "@jupyterlab/mainmenu": "^3.0.0-rc.6",
    "@jupyterlab/settingregistry": "^3.0.0-rc.6",
    "jupyterlab-topbar": "^0.5.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "~4.0.5"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
